<h1>Edit your user info</h1><br>

<%= form_for(@user) do |f| %>
<%= f.text_field :name %><br><br>
<%= f.text_field :username %><br><br>
<%= f.text_field :email %><br><br>
<%= f.text_field :city %><br><br>
<%= f.text_field :bio %><br><br>
<%= f.text_field :years_hiking %><br><br>
<br><label>Admin? Check box if yes: </label><br>
<%= f.check_box :admin %><br><br>
<%= f.submit "Edit user info" %>
<% end %>